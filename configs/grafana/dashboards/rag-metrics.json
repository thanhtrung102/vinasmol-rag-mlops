{
  "title": "RAG System Metrics",
  "uid": "rag-metrics",
  "timezone": "",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (req/min)",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(rag_requests_total[5m])) * 60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1000
              },
              {
                "color": "red",
                "value": 5000
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(rag_requests_total{status=\"error\"}[5m])) / sum(rate(rag_requests_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.01
              },
              {
                "color": "red",
                "value": 0.05
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "P95 Latency",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(rag_request_latency_seconds_bucket[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 3
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Cache Hit Rate",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(rag_cache_hits_total[5m])) / (sum(rate(rag_cache_hits_total[5m])) + sum(rate(rag_cache_misses_total[5m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.2
              },
              {
                "color": "green",
                "value": 0.5
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Request Latency Percentiles",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rag_request_latency_seconds_bucket[5m]))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(rag_request_latency_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(rag_request_latency_seconds_bucket[5m]))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 6,
      "title": "Request Rate by Status",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(rag_requests_total{status=\"success\"}[5m])",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "rate(rag_requests_total{status=\"success_cached\"}[5m])",
          "legendFormat": "Cached",
          "refId": "B"
        },
        {
          "expr": "rate(rag_requests_total{status=\"error\"}[5m])",
          "legendFormat": "Error",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "id": 7,
      "title": "Retrieval Score Distribution",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 17
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rag_retrieval_score_bucket[5m]))",
          "legendFormat": "P50 Score",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.75, rate(rag_retrieval_score_bucket[5m]))",
          "legendFormat": "P75 Score",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(rag_retrieval_score_bucket[5m]))",
          "legendFormat": "P95 Score",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 8,
      "title": "Cache Performance",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 17
      },
      "targets": [
        {
          "expr": "rate(rag_cache_hits_total[5m])",
          "legendFormat": "Cache Hits",
          "refId": "A"
        },
        {
          "expr": "rate(rag_cache_misses_total[5m])",
          "legendFormat": "Cache Misses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    }
  ]
}